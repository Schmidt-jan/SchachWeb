# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~
GET     /                               controllers.HomeController.index

POST    /api/new                        controllers.GameControllerJson.gameNew
GET     /api/status                     controllers.GameControllerJson.status
POST    /api/move                       controllers.GameControllerJson.figureMove
POST    /api/undo                       controllers.GameControllerJson.gameUndo
POST    /api/redo                       controllers.GameControllerJson.gameRedo
POST    /api/convertPawn                controllers.GameControllerJson.convertPawn

GET   /ws   controllers.GameControllerJson.ws